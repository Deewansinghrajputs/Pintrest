<% include ./partials/header.ejs %>
<div class="w-hull min-h-screen bg-zinc-800 px-10 py-8 text-white">
    <div class="card flex flex-wrap gap-10">
        <% posts.forEach(function(elem){ %>
            
            <div class=" card w-80">
                <div class=" w-full h-60 bg-sky-200  rounded-lg object-cover">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="mt-2 text-sm"><%= elem.title %></h5>
                <h6 class="text-xs opacity-500"> @<%=elem.user.username  %></h6>
    
            </div>
       <% }) %>
    </div>
</div>


<% include ./partials/footer.ejs %>